{
    "name": "ingeneo/yii2-saml",
    "description": "Connect Yii 2 application to a Saml Identity Provider for Single Sign On",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "saml",
        "single sign on"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arba Sasmoyo",
            "email": "asasmoyo@student.uns.ac.id"
        },
        {
            "name": "Muhammad Safri Juliardi",
            "email": "ardi93@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "yiisoft/yii2": ">=2.0.13",
        "ingeneo/onelogin-php-saml": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0.*"
    },
    "conflict": {
        "phpunit/php-timer": ">=2"
    },
    "autoload": {
        "psr-4": {
            "ingeneo\\yii2saml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ingeneo\\yii2saml\\tests\\": "tests/"
        }
    }
}
